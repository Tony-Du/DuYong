
-- create table
 create table intdata.kesheng_sdk_play_wrong (
        clientId                               String,
        imei                                   String,
        udid                                   String,
        idfa                                   String,
        idfv                                   String,
        appVersion                             String,
        apppkg                                 String,
        networkType                            String,
        os                                     String,
        appchannel                             String,
        installationID                         String,
        client_ip                              String,
        MG_MSG_TIME                            String,
        Session                                String,
        Subsession                             String,
        Error_Code                             String,
        Failed_Detail_Code                     String,
        MG_MSG_MEDIA_INFO_TYPE                 String,
        MG_MSG_MEDIA_INFO_VIDEO_CODEC          String,
        MG_MSG_MEDIA_INFO_VIDEO_RESOLUTION     String,
        MG_MSG_MEDIA_INFO_VIDEO_FRAMERATE      String,
        sub_networkType                        String,
        MG_MSG_MEDIA_INFO_VIDEO_BITRATE        String,
        MG_MSG_MEDIA_INFO_AUDIO_CODEC          String,
        MG_MSG_MEDIA_INFO_AUDIO_CHANNELS       String,
        MG_MSG_MEDIA_INFO_AUDIO_SAMPLERATE     String,
        SubsessionServiceURL                   String,
        SubsessionServiceIP                    String,
        province_name                          String,
        city_name                              String,
        provider_name                          String
 )
 PARTITIONED BY (src_file_day String, src_file_hour String)
 STORED AS PARQUET;
 
 
-- 第一步：
 set mapreduce.job.name = intdata.kesheng_sdk_play_wrong_${SRC_FILE_DAY}_${SRC_FILE_HOUR};
 
 insert overwrite table intdata.kesheng_sdk_play_wrong partition (src_file_day='${SRC_FILE_DAY}',src_file_hour='${SRC_FILE_HOUR}')
 select s.clientid,                              
        s.imei,                                          
        s.udid,                                          
        s.idfa,                                          
        s.idfv,                                          
        s.appversion,                                    
        s.apppkg,                                        
        s.networktype,                                   
        s.os,                                            
        s.appchannel,                                    
        s.installationid,
        s.client_ip,              
        c.MG_MSG_TIME,
        c.Session,
        c.Subsession,
        c.Error_Code,    
        c.Failed_Detail_Code, 
        c.MG_MSG_MEDIA_INFO_TYPE, 
        c.MG_MSG_MEDIA_INFO_VIDEO_CODEC, 
        c.MG_MSG_MEDIA_INFO_VIDEO_RESOLUTION, 
        c.MG_MSG_MEDIA_INFO_VIDEO_FRAMERATE, 
        c.sub_networkType,
        c.MG_MSG_MEDIA_INFO_VIDEO_BITRATE, 
        c.MG_MSG_MEDIA_INFO_AUDIO_CODEC, 
        c.MG_MSG_MEDIA_INFO_AUDIO_CHANNELS, 
        c.MG_MSG_MEDIA_INFO_AUDIO_SAMPLERATE, 
        c.SubsessionServiceURL, 
        c.SubsessionServiceIP,              
        s.province_name,                                 
        s.city_name,                                     
        s.provider_name           
   from ods.kesheng_sdk_custominfo_json_v s
lateral view json_tuple(s.custominfo_json,
                        'MG_MSG_TIME',
                        'Session',
                        'Subsession',
                        'Error_Code',
                        'Failed_Detail_Code', 
                        'MG_MSG_MEDIA_INFO_TYPE', 
                        'MG_MSG_MEDIA_INFO_VIDEO_CODEC', 
                        'MG_MSG_MEDIA_INFO_VIDEO_RESOLUTION', 
                        'MG_MSG_MEDIA_INFO_VIDEO_FRAMERATE', 
                        'networkType',
                        'MG_MSG_MEDIA_INFO_VIDEO_BITRATE', 
                        'MG_MSG_MEDIA_INFO_AUDIO_CODEC', 
                        'MG_MSG_MEDIA_INFO_AUDIO_CHANNELS', 
                        'MG_MSG_MEDIA_INFO_AUDIO_SAMPLERATE', 
                        'SubsessionServiceURL', 
                        'SubsessionServiceIP') c
     as MG_MSG_TIME,Session,Subsession,Error_Code,Failed_Detail_Code, 
        MG_MSG_MEDIA_INFO_TYPE,MG_MSG_MEDIA_INFO_VIDEO_CODEC,MG_MSG_MEDIA_INFO_VIDEO_RESOLUTION, 
        MG_MSG_MEDIA_INFO_VIDEO_FRAMERATE,sub_networkType,MG_MSG_MEDIA_INFO_VIDEO_BITRATE, 
        MG_MSG_MEDIA_INFO_AUDIO_CODEC,MG_MSG_MEDIA_INFO_AUDIO_CHANNELS,
        MG_MSG_MEDIA_INFO_AUDIO_SAMPLERATE,SubsessionServiceURL,SubsessionServiceIP              
  where s.custominfo_type = '58000000' and src_file_day='${SRC_FILE_DAY}' and src_file_hour='${SRC_FILE_HOUR}';
 
 
 

 
 
 
 
 
 
 
 
 