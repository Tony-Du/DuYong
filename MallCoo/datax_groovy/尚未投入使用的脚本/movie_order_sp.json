{
    "job": {
        "content": [{
            "reader": {
                "name": "mallcoo_mongodbreader",
                "parameter": {
                    "collectionName": "MovieOrder",
                    "column": [
                        { "name": "MallID" },
                        { "name": "Phone" },
                        { "name": "MovieID" },
                        { "name": "MovieName" },
                        { "name": "OrderSource" },
                        { "name": "Price" },
                        { "name": "TotalPrice" },
						{ "name": "OrderNo" },
                        { "name": "CreateTime" },
						{ "name": "BuyCount" },
                        { "name": "CreatorID" }
                    ],
                    "dbName": "SP_Movie",
                    "url":"$url/SP_Movie.MovieOrder$qs",
					"query":"{'CreateTime':{ $gt: $start,$lte: $end }}"
                }
            },
            "writer": {
                "name": "hdfswriter",
                "parameter": {
                    "column": [
                        { "name": "MallID", "type": "bigint" },
                        { "name": "Mobile", "type": "string" },
                        { "name": "MovieID", "type": "bigint" },
                        { "name": "MovieName", "type": "string" },
                        { "name": "OrderSource", "type": "int" },
                        { "name": "Price", "type": "double" },
                        { "name": "TotalPrice", "type": "double" },
						{ "name": "OrderNo", "type": "string" },
                        { "name": "CreateTime", "type": "timestamp" },
						{ "name": "BuyCount", "type": "int" },
                        { "name": "UID", "type": "bigint" }
                    ],
                    "defaultFS": "hdfs://hadoop000:9000",
                    "fieldDelimiter": "\1",
                    "fileName": "$filename",
                    "fileType": "orc",
                    "path": "/user/root/hive/crm.db/movieorder",
                    "writeMode": "append"
                }
            }
        }],
        "setting": { "speed": { "channel": $channel } }
    }
}