{
    "job": {
        "content": [{
            "reader": {
                "name": "mallcoo_mongodbreader",
                "parameter": {
                    "collectionName": "GrouponCoupon",
                    "column": [
                        { "name": "UID" },
                        { "name": "MallID" },
                        { "name": "OrderNo" },
                        { "name": "Price" },
                        { "name": "PayStatus" },
                        { "name": "CreateTime" },
                        { "name": "GrouponID" },
                        { "name": "GrouponName" },
						{ "name": "Status" },
                        { "name": "UsedTime" },
                        { "name": "PayType" },
                        { "name": "UsedShopID" }
                    ],
                    "dbName": "Mallcoo",
                    "url":"$url/Mallcoo.GrouponCoupon$qs",
					"query":"{'CreateTime':{ $gt: $start,$lte: $end }}"
                }
            },
            "writer": {
                "name": "hdfswriter",
                "parameter": {
                    "column": [
                        { "name": "UID", "type": "bigint" },
                        { "name": "MallID", "type": "bigint" },
                        { "name": "OrderNo", "type": "bigint" },
                        { "name": "Price", "type": "double" },
                        { "name": "PayStatus", "type": "int" },
                        { "name": "CreateTime", "type": "timestamp" },
                        { "name": "GrouponID", "type": "bigint" },
                        { "name": "GrouponName", "type": "string" },
						{ "name": "Status", "type": "bigint" },
                        { "name": "UsedTime", "type": "timestamp" },
                        { "name": "PayType", "type": "int" },
                        { "name": "ShopID", "type": "bigint" }
                    ],
                    "defaultFS": "hdfs://hadoop000:9000",
                    "fieldDelimiter": "\1",
                    "fileName": "$filename",
                    "fileType": "orc",
                    "path": "/user/root/hive/online.db/grouponcoupon",
                    "writeMode": "append"
                }
            }
        }],
        "setting": { "speed": { "channel": $channel } }
    }
}