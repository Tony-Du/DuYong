{
    "job": {
        "content": [{
            "reader": {
                "name": "mallcoo_mongodbreader",
                "parameter": {
                    "collectionName": "GiftCoupon",
                    "column": [
                        { "name": "MallID" },
                        { "name": "UID" },
                        { "name": "Mobile" },
                        { "name": "GiftID" },
                        { "name": "GiftTitle" },
                        { "name": "DataSource" },
                        { "name": "Point" },
						{ "name": "ExchangeTime" },
                        { "name": "ReceivedTime" }
                    ],
                    "dbName": "Mallcoo",
                    "url":"$url/Mallcoo.GiftCoupon$qs",
					"query":"{'ExchangeTime':{ $gt: $start,$lte: $end }}"
                }
            },
            "writer": {
                "name": "hdfswriter",
                "parameter": {
                    "column": [
                        { "name": "MallID", "type": "bigint" },
                        { "name": "UID", "type": "bigint" },
                        { "name": "Mobile" "type": "string" },
                        { "name": "GiftID" "type": "bigint" },
                        { "name": "GiftTitle" "type": "string" },
                        { "name": "DataSource" "type": "int" },
                        { "name": "Point" "type": "bigint" },
						{ "name": "ExchangeTime" "type": "timestamp" },
                        { "name": "ReceivedTime" "type": "timestamp" }
                    ],
                    "defaultFS": "hdfs://hadoop000:9000",
                    "fieldDelimiter": "\1",
                    "fileName": "$filename",
                    "fileType": "orc",
                    "path": "/user/root/hive/online.db/giftcoupon",
                    "writeMode": "append"
                }
            }
        }],
        "setting": { "speed": { "channel": $channel } }
    }
}