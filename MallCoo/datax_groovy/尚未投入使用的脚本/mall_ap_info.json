{
    "job": {
        "content": [{
            "reader": {
                "name": "mallcoo_mongodbreader",
                "parameter": {
                    "collectionName": "MallAPInfo",
                    "column": [
                        { "name": "MallId" },
                        { "name": "MallName" },
                        { "name": "SN" },
                        { "name": "Macs", "type": "Array", "splitter": "_" },
                        { "name": "FloorID" },
                        { "name": "X" },
						{ "name": "y" }
                    ],
                    "dbName": "Mallcoo",
                    "url":"$url/Mallcoo.MallAPInfo$qs"
                }
            },
            "writer": {
                "name": "hdfswriter",
                "parameter": {
                    "column": [
                        { "name": "MallId", "type": "bigint" },
                        { "name": "MallName", "type": "string" },
                        { "name": "SN", "type": "string" },
                        { "name": "Macs", "type": "string" },
                        { "name": "FloorID", "type": "bigint" },
                        { "name": "X", "type": "double" },
						{ "name": "y", "type": "double" }
                    ],
                    "defaultFS": "hdfs://hadoop000:9000",
                    "fieldDelimiter": "\1",
                    "fileName": "$filename",
                    "fileType": "orc",
                    "path": "/user/root/hive/wifi.db/mallapinfo",
                    "writeMode": "append"
                }
            }
        }],
        "setting": { "speed": { "channel": $channel } }
    }
}