{
    "job": {
        "content": [{
            "reader": {
                "name": "mallcoo_mongodbreader",
                "parameter": {
                    "collectionName": "AppPageView",
                    "column": [
                        { "name": "_id" },
                        { "name": "UUID" },
                        { "name": "BindID" },
                        { "name": "PVType" },
                        { "name": "ShopID" },
                        { "name": "MallID" },
                        { "name": "AppType" },
                        { "name": "UserID" },
                        { "name": "CreateTime" },
                        { "name": "DataSource" }
                    ],
                    "dbName": "MallcooExt",
                    "url":"$url/MallcooExt.AppPageView$qs",
					"query":"{'CreateTime':{ $gt: $start,$lte: $end }}"
                }
            },
            "writer": {
                "name": "hdfswriter",
                "parameter": {
                    "column": [
                        { "name": "id", "type": "bigint" },
                        { "name": "UUID", "type": "string" },
                        { "name": "BindID", "type": "bigint" },
                        { "name": "PVType", "type": "int" },
                        { "name": "ShopID", "type": "bigint" },
                        { "name": "MallID", "type": "bigint" },
                        { "name": "AppType", "type": "int" },
                        { "name": "UserID", "type": "bigint" },
                        { "name": "CreateTime", "type": "timestamp" },
                        { "name": "DataSource", "type": "int" }
                    ],
                    "defaultFS": "hdfs://hadoop000:9000",
                    "fieldDelimiter": "\1",
                    "fileName": "$filename",
                    "fileType": "orc",
                    "path": "/user/root/hive/online.db/apppageview",
                    "writeMode": "append"
                }
            }
        }],
        "setting": { "speed": { "channel": $channel } }
    }
}