{
    "job": {
        "content": [{
            "reader": {
                "name": "mallcoo_mongodbreader",
                "parameter": {
                    "collectionName": "PPInfo",
                    "column": [
                        { "name": "MallID" },
                        { "name": "UID" },
                        { "name": "Mobile" },
                        { "name": "IsPromotion" },
                        { "name": "PromotionID" },
                        { "name": "PromotionName" },
                        { "name": "Status" },
                        { "name": "Money" },
						{ "name": "UsedTime" },
                        { "name": "IsMallActivity" },
                        { "name": "CreateTime" }
                    ],
                    "dbName": "Mallcoo",
                    "url":"$url/Mallcoo.PPInfo$qs",
					"query":"{'CreateTime':{ $gt: $start,$lte: $end }}"
                }
            },
            "writer": {
                "name": "hdfswriter",
                "parameter": {
                    "column": [
                        { "name": "MallID", "type": "bigint" },
                        { "name": "UID", "type": "bigint" },
                        { "name": "Mobile", "type": "string" },
                        { "name": "IsPromotion", "type": "int" },
                        { "name": "PromotionID", "type": "bigint" },
                        { "name": "PromotionName", "type": "string" },
                        { "name": "Status", "type": "int" },
                        { "name": "Money", "type": "double" },
						{ "name": "UsedTime", "type": "timestamp" },
                        { "name": "IsMallActivity", "type": "BOOLEAN" },
                        { "name": "CreateTime", "type": "timestamp" }
                    ],
                    "defaultFS": "hdfs://hadoop000:9000",
                    "fieldDelimiter": "\1",
                    "fileName": "$filename",
                    "fileType": "orc",
                    "path": "/user/root/hive/online.db/ppinfo",
                    "writeMode": "append"
                }
            }
        }],
        "setting": { "speed": { "channel": $channel } }
    }
}