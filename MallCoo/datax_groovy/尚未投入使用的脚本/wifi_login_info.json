{
    "job": {
        "content": [{
            "reader": {
                "name": "mallcoo_mongodbreader",
                "parameter": {
                    "collectionName": "WifiLoginInfo",
                    "column": [
                        { "name": "_id" },
                        { "name": "MallID" },
                        { "name": "Mobile" },
                        { "name": "Mac" },
                        { "name": "LoginTime" },
                        { "name": "CreateTime" }
                    ],
                    "dbName": "MallcooExt",
                    "url":"$url/MallcooExt.WifiLoginInfo$qs",
					"query":"{'CreateTime':{ $gt: $start,$lte: $end }}"
                }
            },
            "writer": {
                "name": "hdfswriter",
                "parameter": {
                    "column": [
                        { "name": "ID", "type": "bigint" },
                        { "name": "MallID", "type": "bigint" },
                        { "name": "Mobile", "type": "string" },
                        { "name": "Mac", "type": "string" },
                        { "name": "LoginTime", "type": "timestamp" },
                        { "name": "CreateTime", "type": "timestamp" }
                    ],
                    "defaultFS": "hdfs://hadoop000:9000",
                    "fieldDelimiter": "\1",
                    "fileName": "$filename",
                    "fileType": "orc",
                    "path": "/user/root/hive/wifi.db/wifilogininfo",
                    "writeMode": "append"
                }
            }
        }],
        "setting": { "speed": { "channel": $channel } }
    }
}