{
    "job": {
        "content": [{
            "reader": {
                "name": "mallcoo_mongodbreader",
                "parameter": {
                    "collectionName": "LotteryRecord",
                    "column": [
                        { "name": "MallID" },
                        { "name": "BIID" },
                        { "name": "BIName" },
                        { "name": "LotteryType" },
                        { "name": "AwardType" },
                        { "name": "CreateTime" },
                        { "name": "ASID" },
						{ "name": "ASName" },
                        { "name": "UserID" },
                        { "name": "Mobile" },
						{ "name": "DataSource" }
                    ],
                    "dbName": "Lottery",
                    "url":"$url/Lottery.LotteryRecord$qs",
					"query":"{'CreateTime':{ $gt: $start,$lte: $end }}"
                }
            },
            "writer": {
                "name": "hdfswriter",
                "parameter": {
                    "column": [
                        { "name": "MallID", "type": "bigint" },
                        { "name": "BIID", "type": "bigint" },
                        { "name": "BIName", "type": "string" },
                        { "name": "LotteryType", "type": "int" },
                        { "name": "AwardType", "type": "int" },
                        { "name": "CreateTime", "type": "timestamp" },
                        { "name": "ASID", "type": "bigint" },
						{ "name": "ASName", "type": "string" },
                        { "name": "UserID", "type": "bigint" },
                        { "name": "Mobile", "type": "bigint" },
						{ "name": "DataSource", "type": "int" }
                    ],
                    "defaultFS": "hdfs://hadoop000:9000",
                    "fieldDelimiter": "\1",
                    "fileName": "$filename",
                    "fileType": "orc",
                    "path": "/user/root/hive/online.db/lotteryrecord",
                    "writeMode": "append"
                }
            }
        }],
        "setting": { "speed": { "channel": $channel } }
    }
}